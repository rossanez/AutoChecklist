$NUMBER = "/$INCLUDE_NUMBERS(($INCLUDE_PREFIXES)?($INCLUDE_UNITS))?/"
$NUMBER_AND_UNIT = "/($INCLUDE_NUMBERS)/ /(($INCLUDE_PREFIXES)?($INCLUDE_UNITS))([\/\-\.]($INCLUDE_PREFIXES)?($INCLUDE_UNITS))?(\.)?/"
$NUMBER_AND_UNIT_CAT = "/($INCLUDE_NUMBERS)(($INCLUDE_PREFIXES)?($INCLUDE_UNITS))([\/\-\.]($INCLUDE_PREFIXES)?($INCLUDE_UNITS))?(\.)?/"
{
   ruleType: "tokens",
   pattern: ( /should/ ),
   result: "SHOULD"
}
{
   ruleType: "tokens",
   pattern: ( /might/ ),
   result: "MIGHT"
}
{
   ruleType: "tokens",
   pattern: ( /can/ ),
   result: "CAN"
}
{
   ruleType: "tokens",
   pattern: ( /should/ ),
   result: "SHOULD"
}
{
   ruleType: "tokens",
   pattern: ( $NUMBER_AND_UNIT ),
   result: "NUMBER_AND_UNIT"
}
{
   ruleType: "tokens",
   pattern: ( $NUMBER_AND_UNIT_CAT ),
   result: "NUMBER_AND_UNIT"
}
{
   ruleType: "tokens",  
   pattern: ( /in/ /preference/ ),
   result: "IN_PREFERENCE"
}
{
   ruleType: "tokens",  
   pattern: ( /a/ /few/ []{0,0} $NUMBER ),
   result: "A_FEW"
}
{
   ruleType: "tokens",
   pattern: ( /in/ /some/ []{0,0} $NUMBER ),
   result: "IN_SOME"
}
{
   ruleType: "tokens",
   pattern: ( /in/ /less/ /than/ []{0,0} $NUMBER ),
   result: "IN_LESS_THAN"
}
{
   ruleType: "tokens",
   pattern: ( /in/ /more/ /than/ []{0,0} $NUMBER ),
   result: "IN_MORE_THAN"
}